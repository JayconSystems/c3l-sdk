config system
	option timezone 'UTC'


config 'timeserver' 'ntp'
        list 'server'     '0.c3wireless.pool.ntp.org'
        list 'server'     '1.c3wireless.pool.ntp.org'
        list 'server'     '2.c3wireless.pool.ntp.org'
        list 'server'     '3.c3wireless.pool.ntp.org'
        option enable_server 0
